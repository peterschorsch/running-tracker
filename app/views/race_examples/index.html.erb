<p id="notice"><%= notice %></p>

<h1>Race Examples</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @race_examples.each do |race_example| %>
      <tr>
        <td><%= link_to 'Show', race_example %></td>
        <td><%= link_to 'Edit', edit_race_example_path(race_example) %></td>
        <td><%= link_to 'Destroy', race_example, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Race Example', new_race_example_path %>
