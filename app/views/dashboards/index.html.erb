<p class="text-right"><%= formatFullDate(DateTime.now) %></p>
<hr />
<%= render notice_path %>
<%= render alert_path %>

<div class="row">
	<div class="col-md-4">
		<%= render "dashboards/panels/last-run-panel", last_run: @last_run %>
	</div>
	<div class="col-md-4">
		<%= render "dashboards/panels/next-run-panel", next_run: @next_run %>
	</div>
	<div class="col-md-4">
		<%= render "dashboards/panels/weekly-total-panel", weekly_total: @weekly_total %>
	</div>
</div>

<div class="row">
	<div class="col-md-8">
		<%= render "dashboards/panels/line-chart-panel", weekly_total_graph: @weekly_total_graph %>
	</div>
	<div class="col-md-4">
		<%= render "dashboards/panels/monthly-yearly-totals-panel", monthly_total: @monthly_total, yearly_total: @yearly_total %>
	</div>
</div>