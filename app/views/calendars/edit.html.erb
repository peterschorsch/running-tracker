<%= render edit_header_path, path: calendars_path, btn_text: "Calendar" %>

<div class="row">
	<div class="col-md-10 col-md-offset-1">
		<div class="panel panel-default">
			<div class="panel-heading text-center">
				<h1>Editing: <strong><%= @run.name %></strong></h1>
			</div>
			<div class="panel-body">
				<%= render "runs/form", run: @run, action: "update", disable_form: @run.determine_run_modification_by_user_role %>
			</div
		</div>
	</div>
</div>
