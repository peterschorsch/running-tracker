<%= render edit_header_path, path: admin_race_examples_path, btn_text: "Race Examples" %>

<div class="row">
	<div class="col-md-10 col-md-offset-1">
		<div class="panel panel-default">
			<div class="panel-heading text-center">
				<h1>Editing: <%= bold_text(@race_example.name) %></h1>
			</div>
			<div class="panel-body">
				<%= render 'form', race_example: @race_example, disable_form: session[:website_viewer] %>
			</div>
		</div>
	</div>
</div>
