<%= render index_header_path, heading_text: "Race Pace Chart" %>

<table class="table table-responsive table-striped table-bordered table-hover make-text-unselectable">
	<thead>
		<% @race_paces.each do |header, data| %>
			<tr>
				<% (0..header.count-1).each do |index| %>
					<th><%= header[index] %></th>
				<% end %>
			</tr>
	</thead>
	<tbody>
			<% data.each_with_index do |body, index| %>
				<tr>
					<th><%= body[0] %></th>
					<% (1..body.count-1).each do |index| %>
						<td><%= body[index] %></td>
					<% end %>
				</tr>
			<% end %>
		<% end %>
	</tbody>
</table>